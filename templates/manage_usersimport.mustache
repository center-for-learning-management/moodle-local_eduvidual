<div class="local_eduvidual manage_usersimport {{uniqid}}" data-orgid="{{orgid}}">
    <a href="#" class="btn btn-primary btn-block import-btn-{{uniqid}}"
        onclick="require(['local_eduvidual/manager'], function(M) { M.createUsers('{{uniqid}}'); }); return false;">
        {{#str}} manage:createuserspreadsheet:import:datavalidated, local_eduvidual {{/str}}
    </a>
    <div style="width: 100%; overflow-y: scroll;">
        <table class="generaltable" width="100%">
            <tr>
                <th>{{#str}} manage:createuserspreadsheet:col:check, local_eduvidual {{/str}}</th>
                <th>{{#str}} manage:createuserspreadsheet:col:result, local_eduvidual {{/str}}</th>
                <th>{{#str}} firstname {{/str}}</th>
                <th>{{#str}} lastname {{/str}}</th>
                <th>{{#str}} email {{/str}}</th>
                <th>{{#str}} role {{/str}}</th>
                <th>{{#str}} manage:createuserspreadsheet:col:cohorts_add, local_eduvidual {{/str}}</th>
                <th>{{#str}} manage:createuserspreadsheet:col:cohorts_remove, local_eduvidual {{/str}}</th>
                <th>{{#str}} password {{/str}}</th>
                <th>{{#str}} forcechangepassword, auth {{/str}}</th>
            </tr>
            {{#users}}
            <tr class="user" data-userid="{{id}}">
                <td align="center">
                    <img class="icon process-indicator" src="/pix/i/completion-auto-n.svg" />
                </td>
                <td class="action">{{payload.action}}</td>
                <td class="firstname">{{firstname}}</td>
                <td class="lastname">{{lastname}}</td>
                <td class="email">{{email}}</td>
                <td class="role">{{role}}</td>
                <td class="cohorts_add">{{cohorts_add}}</td>
                <td class="cohorts_remove">{{cohorts_remove}}</td>
                <td class="password">{{password}}</td>
                <td class="forcechangepassword">{{forcechangepassword}}</td>
            </tr>
            {{/users}}
        </table>
    </div>
    <a href="#" class="btn btn-primary btn-block import-btn-{{uniqid}}"
        onclick="require(['local_eduvidual/manager'], function(M) { M.createUsers('{{uniqid}}'); }); return false;">
        {{#str}} manage:createuserspreadsheet:import:datavalidated, local_eduvidual {{/str}}
    </a>
</div>
